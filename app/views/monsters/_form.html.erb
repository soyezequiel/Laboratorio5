<%= form_with model: @monsterD do |fa| %>
<%= fa.search_field  :name, placeholder: "nombre", size:"30x1" %> <br>
<%= fa.text_area :description, placeholder: "Descripcion" %><br>
<%= fa.telephone_field :phone, placeholder: "telefono"%><br>
<%= fa.datetime_local_field  :birthdate, placeholder: "fecha de nacimiento"%>
<%= fa.submit "editar" %>
<%end%>